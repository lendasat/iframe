{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            n.id,\n            n.installment_id,\n            n.contract_id,\n            n.status as \"status: InstallmentStatus\",\n            n.read,\n            n.created_at,\n            n.updated_at\n        FROM lender_installment_update_notifications n\n        INNER JOIN contracts c ON n.contract_id = c.id\n        WHERE c.lender_id = $1\n        AND ($2 = false OR n.read = false)\n        ORDER BY n.created_at DESC\n        LIMIT $3 OFFSET $4\n        ",
