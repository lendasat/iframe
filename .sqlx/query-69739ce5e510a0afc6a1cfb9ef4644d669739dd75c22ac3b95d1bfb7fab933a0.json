  "query": "\n            SELECT\n                id,\n                contract_id,\n                principal,\n                interest,\n                due_date,\n                status AS \"status: InstallmentStatus\",\n                paid_date,\n                payment_id\n            FROM installments\n            WHERE\n                status = 'Pending' AND\n                due_date > $1 AND\n                due_date <= $2\n        ",
