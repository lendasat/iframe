{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            n.id,\n            n.contract_id,\n            n.read,\n            n.created_at,\n            n.updated_at,\n            b.name as \"counterparty_name!\"\n        FROM lender_contract_chat_message_notifications n\n        INNER JOIN contracts c ON n.contract_id = c.id\n        INNER JOIN borrowers b ON c.borrower_id = b.id\n        WHERE c.lender_id = $1\n        ORDER BY n.created_at DESC\n        ",
