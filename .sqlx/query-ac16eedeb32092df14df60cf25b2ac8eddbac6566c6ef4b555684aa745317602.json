{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            id,\n            created_at,\n            updated_at,\n            txid,\n            amount_sats,\n            amount_usd,\n            installment_id,\n            address,\n            expires_at,\n            status AS \"status: BitcoinInvoiceStatus\"\n        FROM btc_invoices\n        WHERE installment_id = $1\n            AND status = 'Pending'\n            AND expires_at > $2\n        ORDER BY created_at ASC\n        LIMIT 1\n        ",
