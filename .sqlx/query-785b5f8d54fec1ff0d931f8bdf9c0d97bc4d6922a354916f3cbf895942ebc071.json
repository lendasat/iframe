  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            f.id,\n            f.name,\n            f.description,\n            COALESCE(bff.is_enabled, f.enabled) as \"is_enabled!: bool\"\n        FROM lender_features f\n        LEFT JOIN lender_feature_flags bff \n            ON bff.feature_id = f.id \n            AND bff.lender_id = $1\n        ",
