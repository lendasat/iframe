  "query": "\n            UPDATE\n                contracts\n            SET\n                status = $1, updated_at = $2\n            WHERE\n                created_at + (duration_months * INTERVAL '1 month') <= $2 AND\n                status NOT IN ($3, $4, $5, $6, $7, $8, $9, $1)\n            RETURNING id;\n        ",
