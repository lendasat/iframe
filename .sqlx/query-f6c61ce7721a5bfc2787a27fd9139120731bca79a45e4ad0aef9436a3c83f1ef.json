  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            lo.id as \"id!\",\n            lo.loan_amount_min,\n            lo.loan_amount_max,\n            lo.interest_rate,\n            lo.duration_days_min,\n            lo.duration_days_max,\n            lo.loan_asset::text as \"loan_asset!\",\n            lo.created_at\n        FROM loan_offers lo\n        WHERE lo.status = 'Available'\n          AND lo.created_at >= $1\n        ORDER BY lo.created_at DESC\n        ",
