{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            f.id,\n            f.name,\n            f.description,\n            COALESCE(bff.is_enabled, f.enabled) as \"is_enabled!: bool\"\n        FROM features f\n        LEFT JOIN borrower_feature_flags bff \n            ON bff.feature_id = f.id \n            AND bff.borrower_id = $1\n        ",
